<Window x:Class="CMG.License.Analytics.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="CMG Log File Analytics Tool"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:regions="clr-namespace:CMG.License.Shared.PrismHelpers;assembly=CMG.License.Shared"
        >
    <Grid>
        <ContentControl 
            prism:RegionManager.RegionName="{Binding Name, Source={x:Static regions:PrismRegions.MainRegion}}" />
    </Grid>
</Window>